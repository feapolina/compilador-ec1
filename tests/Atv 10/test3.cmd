fun soma(var a, var b) {
    var resultado = a + b;
    return resultado;
}
fun subtra(var a, var b) {
	var resultado = a - b;
	return resultado
}
fun multi(var a, var b) {
	var resultado = a * b;
	return resultado
}

fun main() {
    var x = 5;
    var y = 10;
	var z = multi(x, y);
    return z;
}

